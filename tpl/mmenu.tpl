<ul class="mmenu">{foreach from=$mmenuArray key=key item=item}<li><a href="{$item.url}" target="{$item.target}"{if $item.quick_desc neq ''} title="{$item.quick_desc|escape}"{/if} class="{$item.active}"{if isset($item.inAirAdminMode)} data-action="{$item.id}"{/if}>{$item.name}{if isset($item.icon)}<span class="coloranimate"></span><span style="background:url({$item.icon});"></span>{/if}</a>{include file="mmenu.tpl" mmenuArray=$item.childNodes}</li>{/foreach}</ul>